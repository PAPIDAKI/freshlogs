
<br>

<div class='raw'>
  <div>
    <h1>Listing Pmus</h1>
  </div>
  <div>
    <%= link_to 'New Pmu', new_tenant_project_pmu_path(@tenant,@project) %>
  </div>
</div>
<table>
  <thead>
    <tr>
      <th>Produce</th>
      <th>Village</th>
      <th>Location</th>
      <th>Size</th>
      <th>Plants</th>
      <th>Production</th>
      <th>Facilities</th>
      <th>Certification</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pmus.each do |pmu| %>
      <tr>
        <td><%= pmu.produce %></td>
        <td><%= pmu.village %></td>
        <td><%= pmu.location %></td>
        <td><%= pmu.size %></td>
        <td><%= pmu.plants %></td>
        <td><%= pmu.production %></td>
        <td><%= pmu.facilities %></td>
        <td><%= pmu.certification %></td>
        <td><%= pmu.latitude %></td>
        <td><%= pmu.longitude %></td>
        <td><%= link_to 'Show',tenant_project_pmu_path(@tenant,@project,pmu)%></td>
        <td><%= link_to 'Edit', edit_tenant_project_pmu_path(@tenant,@project,pmu) %></td>
       <% #TODO: add destroy path to destroy link%>
        <td><%= link_to 'Destroy',tenant_project_pmu_path(@tenant,@project,pmu),method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>





