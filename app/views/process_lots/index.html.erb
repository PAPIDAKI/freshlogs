<p id="notice"><%= notice %></p>

<h1>Listing Process Lots</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Lot</th>
      <th>Quantity</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @process_lots.each do |process_lot| %>
      <tr>
        <td><%= process_lot.date %></td>
        <td><%= process_lot.lot %></td>
        <td><%= process_lot.quantity %></td>
        <td><%= process_lot.note %></td>
        <td><%= link_to 'Show', process_lot %></td>
        <td><%= link_to 'Edit', edit_process_lot_path(process_lot) %></td>
        <td><%= link_to 'Destroy', process_lot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Process lot', new_process_lot_path %>
