<p id="notice"><%= notice %></p>

<h1>Listing Shift Lists</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Tenant</th>
      <th>Start at</th>
      <th>End at</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shift_lists.each do |shift_list| %>
      <tr>
        <td><%= shift_list.course_id %></td>
        <td><%= shift_list.tenant_id %></td>
        <td><%= shift_list.start_at %></td>
        <td><%= shift_list.end_at %></td>
        <td><%= shift_list.note %></td>
        <td><%= link_to 'Show', shift_list %></td>
        <td><%= link_to 'Edit', edit_shift_list_path(shift_list) %></td>
        <td><%= link_to 'Destroy', shift_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shift list', new_shift_list_path %>
