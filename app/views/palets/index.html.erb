<h1>Listing the Palets</h1>
<%= link_to 'New Palet',new_tenant_palet_path(@tenant) %>

<div class="media">
  <div class='media-body'>
    <table class='table table-hover'>
      <thead>
        <tr>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody> 
        <% @palets.each do |palet| %>
          <tr> 
            <td><%= palet.date.strftime("%e %b %H:%y") %></td>
            <td><%=link_to palet.code,tenant_palet_path(@tenant,palet)%></td>
            <td><%=palet.note%></td>
            
           
          </tr>
        <% end %>
      </tbody>
    </table>
        <br>
    </div>
</div>
    <%= link_to 'New Palet',new_tenant_palet_path(@tenant) %>
